{
  "name": "digital-signing",
  "displayName": "Digital Signing",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./build/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "digital-signing.sign",
        "title": "Sign"
      },
      {
        "command": "digital-signing.verify",
        "title": "Verify"
      },
      {
        "command": "digital-signing.generateKeyPair",
        "title": "Generate Key Pair"
      }
    ],
    "configuration": {
      "title": "Digital Signing",
      "properties": {}
    }
  },
  "activationEvents": [
    "onCommand:digital-signing.sign",
    "onCommand:digital-signing.verify",
    "onCommand:digital-signing.generateKeyPair"
  ]
}
